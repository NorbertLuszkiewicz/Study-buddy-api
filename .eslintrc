{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": 0,
    "consistent-return": 0,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "import/extensions": ["error", "ignorePackages", { "js": "always" }]
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  }
}
